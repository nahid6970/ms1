{
    "manifest_version": 3,
    "name": "Google Voice Search",
    "version": "1.0",
    "description": "Instant voice search for Google.",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Click to Speak"
    },
    "options_page": "options.html"
}