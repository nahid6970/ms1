{
  "manifest_version": 3,
  "name": "My Script Manager",
  "version": "1.0",
  "description": "A Chrome extension to manage and inject custom scripts.",
  "permissions": [
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "assets/icons/fdm128.png",
      "16": "assets/icons/fdm16.png",
      "48": "assets/icons/fdm48.png"
    }
  },
   "icons": {
      "128": "assets/icons/fdm128.png",
      "16": "assets/icons/fdm16.png",
      "48": "assets/icons/fdm48.png"
   }
}